{
  "extends": "@configuration/tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./src",
    "declaration": true,
    "allowJs": true,
    "declarationDir": "./dist",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    },
    "typeRoots": [
      "node_modules/@grpc/**/*",
      "node_modules/@types/**/*",
      "src/**/*"
    ]
  }
}
