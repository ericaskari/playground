{
  "name": "fullstack-ericaskari",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose up --build --remove-orphans",
    "Start:App.Backend.Server": "PORT=8001 nx serve App.Backend.Server -- --no-inspect",
    "Start:App.Frontend.Web": "nx serve App.Frontend.Web --port 8442 --host 0.0.0.0 --public-host local.ericaskari.com",
    "Build:App.Backend.Server": "nx build App.Backend.Server --prod",
    "Build:App.Frontend.Web": "nx build App.Frontend.Web --prod",
    "Test:App.Backend.Server": "nx test App.Backend.Server",
    "Test:App.Frontend.Web": "nx test App.Frontend.Web",
    "Build:Tools.Github.Actions": "npm run build Tools.Github.Actions --no-cache",
    "Compile:Lib.Frontend.Web3": "truffle compile",
    "Migrate:Lib.Frontend.Web3": "truffle migrate",
    "Migrate:Reset:Lib.Frontend.Web3": "truffle migrate --reset",
    "build": "nx build",
    "test": "nx test",
    "postinstall": "ngcc --properties es2015 browser module main",
    "watch:Tools.Github.Actions": "nodemon --watch './Tools.Github.Actions/src/actions' --ext '*' --exec 'npm run Build:Tools.Github.Actions'",
    "ncc-extract-from-published-release": "./github-action-watch.sh extract-from-published-release"
  },
